{
  "name": "pokedex",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Irvan Adhitya",
  "scripts": {
    "develop:server": "BABEL_ENV=server nodemon .",
    "develop:client": "webpack-dev-server --progress --color --config ./webpack.config.dev.js",
    "develop": "PORT=3000 yarn develop:client & PORT=3000 API_ENDPOINT=https://graphql-pokemon2.vercel.app yarn develop:server",
    "build:server": "BABEL_ENV=server babel --out-dir build src --copy-files",
    "build:client": "webpack --progress --config ./webpack.config.prod.js",
    "build": "yarn build:server && NODE_ENV=production yarn build:client",
    "lint": "eslint --ext .js --ext .jsx src/ && stylelint  \"src/**/*.css\"",
    "prepush": "yarn lint"
  },
  "moduleRoots": [
    "src/app",
    "assets",
    "src/client",
    "src/server",
    "src/config"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "babel-runtime": "^6.26.0",
    "debounce": "^1.2.0",
    "ejs": "^2.6.1",
    "express": "^4.17.3",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^3.0.1",
    "http-proxy-middleware": "^0.18.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.4.1",
    "react-apollo": "^2.5.3",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-import-graphql": "^2.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-assets-import-to-string": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "husky": "^1.3.1",
    "nodemon": "^1.18.3",
    "postcss": "^6.0.23",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-order": "^0.8.1",
    "webpack": "^3.12.0",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-dev-server": "^2.9.5",
    "webpack-split-by-path": "^2.0.0"
  },
  "repository": "https://github.com/bisoelpetjah/pokedex",
  "license": "MIT"
}
